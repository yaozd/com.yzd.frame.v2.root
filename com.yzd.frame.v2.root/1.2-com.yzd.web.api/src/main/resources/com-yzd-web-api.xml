<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <!-- 消费方应用名，用于计算依赖关系，不是匹配条件，不要与提供方一样 -->
    <dubbo:application name="com.hb.insure.app.api.service.consumer" version="${dubbo.package.timestamp}" logger="slf4j"/>
    <context:property-placeholder location="classpath:/dubbo.properties"/>
    <dubbo:registry  id="defaultRegister" protocol="zookeeper"  address="${dubbo.registry.address}" group="${dubbo.registry.group}" file="${dubbo.application.name}.registry.cache" />
    <!--check="false":dubbo接口懒惰,惰性检查,懒加载,接口检查,服务检查.开启.启用,打开-->
    <!--timeout="10000":远程调用的超时时间为10秒-->
    <dubbo:consumer check="false" filter="mdcFilter"/>
